{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/vivawatch-backend-application/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/vivawatch-backend-application/tsconfig.app.json"
  },
  "projects": {
    "client-partner": {
      "type": "application",
      "root": "apps/client-partner",
      "entryFile": "main",
      "sourceRoot": "apps/client-partner/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-partner/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "platform-partner": {
      "type": "application",
      "root": "apps/platform-partner",
      "entryFile": "main",
      "sourceRoot": "apps/platform-partner/src",
      "compilerOptions": {
        "tsConfigPath": "apps/platform-partner/tsconfig.app.json"
      }
    },
    "vivawatch-backend-application": {
      "type": "application",
      "root": "apps/vivawatch-backend-application",
      "entryFile": "main",
      "sourceRoot": "apps/vivawatch-backend-application/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vivawatch-backend-application/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/vivawatch-backend-application"
}